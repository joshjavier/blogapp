{
  "name": "blogapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "start": "NODE_ENV=production node src/backend/index.js",
    "backend:dev": "NODE_ENV=development nodemon --env-file=.env.local src/backend/index.js",
    "backend:test": "NODE_ENV=test node --env-file=.env.local --test \"src/backend/**/*.test.js\"",
    "backend:test-start": "NODE_ENV=test node --env-file=.env.local src/backend/index.js"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.1.3",
    "morgan": "^1.10.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.6.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "jsdom": "^24.0.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "vite": "^4.4.5",
    "vitest": "^1.3.1"
  }
}
